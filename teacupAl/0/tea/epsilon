FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0.01";
    object      epsilon;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -3 0 0 0 0];

internalField   uniform 0.001;

boundaryField
{
    tea_to_cup
    {
		  type            epsilonWallFunction;
		  value           uniform 0.001;
    }
    tea_to_fluid
    {
		  type            epsilonWallFunction;
		  value           uniform 0.001;
    }

}

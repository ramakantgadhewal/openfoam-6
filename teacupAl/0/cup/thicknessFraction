FoamFile
{
    version     2.0;
    format      ascii;
    arch        "LSB;label=32;scalar=64";
    class       volScalarField;
    location    "0/cup";
    object      thicknessFraction;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    bottom
    {
        type            fixedValue;
        value           uniform 0;
    }
    cupbottom
    {
        type            fixedValue;
        value           uniform 0;
    }
    cup_to_tea
    {
        type            calculated;
        value           uniform 0;
    }
    cup_to_fluid
    {
        type            calculated;
        value           uniform 0;
    }
}



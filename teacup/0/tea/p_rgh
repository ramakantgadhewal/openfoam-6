FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0/fluid";
    object      p_rgh;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [ 1 -1 -2 0 0 0 0 ];

internalField   uniform 0;

boundaryField
{
	tea_to_cup
	{
		type            fixedFluxPressure;
		value           $internalField;
	}
	tea_to_fluid
	{
		type            fixedFluxPressure;
		value           $internalField;
	}
}
